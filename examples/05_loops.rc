print '--- 05 LOOPS ---'

print '1. While Loop:'
count = 3
while count > 0 [
    print 'Countdown: {count}'
    count -= 1
]

print '2. Nested For Loops:'
for row 1 3 [
    for col 1 3 [
        print 'Cell: {row}, {col}'
    ]
]

print '--- END ---'
